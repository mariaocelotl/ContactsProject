@model ZContactos.Models.Usuario

@{
    ViewData["Title"] = "Detalles de contacto";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1 class="text-center"> Contactos</h1>
<hr />

<div class="row">
    <div class="col-12">
        <div class="card border">
            <div class="card-header">
                <h3> <strong>Detalle contacto</strong></h3>
            </div>

        </div>
        <div class="card-body">
           <ul class="list-group">
               <li class="list-group-item"><strong>Nombre: </strong>@Html.DisplayFor(m => m.nombre)</li>
               <li class="list-group-item"><strong>Apellido Paterno: </strong>@Html.DisplayFor(m => m.apellidoPaterno)</li>
               <li class="list-group-item"><strong>Apellid Materno: </strong>@Html.DisplayFor(m => m.apellidoMaterno)</li>
               <li class="list-group-item"><strong>Telefono: </strong>@Html.DisplayFor(m => m.telefono)</li>
               <li class="list-group-item"><strong>Celular: </strong>@Html.DisplayFor(m => m.celular)</li>
               <li class="list-group-item"><strong>Correo: </strong>@Html.DisplayFor(m => m.correo)</li>
           </ul>
        </div>
    </div>
</div>
<div>
    <a asp-action="Index">Regresar a la lista</a>
</div>


